@model vms.Models.VolunteerViewModel

<div class="panel panel-default">

    <table class="table table-hover">
        <thead>
            <tr>
                <td>&#x2714;</td>
                <td>FirstName</td>
                <td>LastName</td>
            </tr>
        </thead>

        @foreach (var item in Model.Volunteers)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
            </tr>

        }
    </table>
</div>
